<div class="container">
  <h1>Restaurants</h1>
  <ul>
    <% @restaurants.each do |restaurant|%>
    <li>
      <p class="card-text"><%= link_to restaurant.name, restaurant_path(restaurant) %></p>
    </li>
    <% end %>
  </ul>
<%= link_to "Add a new restaurant", new_restaurant_path, class:"btn btn-primary" %>

</div>
